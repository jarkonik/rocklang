sieve = (n) => {
	i = 0
	prime = vecnew()
	while i < n {
		prime = vecset(prime, i, 1)
		i = i + 1
	}

	p = 2

	while p * p <= n {
		if vecget(prime, p) == 1 {
			i = p * p
			while i <= n {
				prime = vecset(prime, i, 0)
				i = i + p
			}
		}

		p = p + 1
	}

	p = 2

	while p < n {
		if vecget(prime, p) == 1 {
			print(string(p))
			print("\n")
		}

		p = p + 1
	}
}

sieve(1000)
